{{ form_start(form, {attr: {id: form.vars.id}}) }}
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.month) }}
        </div>
        <div class="col-md-6">
            <div class="row gx-3">
                <div class="col-xl-6">
                    {{ form_row(form.value) }}
                </div>
                <div class="col-xl-6">
                    {{ form_row(form.valueDecimal) }}
                </div>
            </div>
        </div>
    </div>
{{ form_end(form) }}
